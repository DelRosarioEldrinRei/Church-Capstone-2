									}

									var houseblessingdays = data.houseblessing.var_availabledays.split(',');
									var houseblessingavailabledays=[];
										for(i=0; i<houseblessingdays.length; i++){
											if(houseblessingdays[i]==0){houseblessingavailabledays.push(' Sunday')}
											if(houseblessingdays[i]==1){houseblessingavailabledays.push(' Monday')}
