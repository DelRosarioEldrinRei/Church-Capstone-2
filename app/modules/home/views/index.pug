	script(type='text/javascript').
		$(document).ready(function(){
			$('.inquireButtonnew').click(function(){
				var idNow = $(this).val();
				console.log(idNow)
				
						if(idNow== "Marriage"){
							window.location='/index/marriagedetails'
						}//if marriage
								
						if (idNow== 'Blessing Request')	{
							$.post('/index/blessingrequest')
								.done(data =>{
									console.log(data.anointing)
									console.log(data.funeralmass)
									console.log(data.funeralservice)
									console.log(data.houseblessing)

									var anointdays = data.anointing.var_availabledays.split(',');
