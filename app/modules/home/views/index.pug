											if(funeralservicedays[i]==0){funeralserviceavailabledays.push(' Sunday')}
											if(funeralservicedays[i]==1){funeralserviceavailabledays.push(' Monday')}
											if(funeralservicedays[i]==2){funeralserviceavailabledays.push(' Tuesday')}
