											if(anointdays[i]==2){anointavailabledays.push(' Tuesday')}
											if(anointdays[i]==3){anointavailabledays.push(' Wednesday')}
											if(anointdays[i]==4){anointavailabledays.push(' Thursday')}
											if(anointdays[i]==5){anointavailabledays.push(' Friday')}
											if(anointdays[i]==6){anointavailabledays.push(' Saturday')}
									}
									var funeralmassdays = data.funeralmass.var_availabledays.split(',');
									var funeralmassavailabledays=[];
										for(i=0; i<funeralmassdays.length; i++){
											if(funeralmassdays[i]==0){funeralmassavailabledays.push(' Sunday')}
