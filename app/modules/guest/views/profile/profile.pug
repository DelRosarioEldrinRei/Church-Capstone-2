extends ../../../../templates/guests
block main
	style.
		@import url('https://fonts.googleapis.com/css?family=Montserrat:200,300');

		body {
			background: #ececec;
		}

		#header-style-1 {
				height:350px;
				background-image: url(../img/samplecover.jpg) !important;
		}
		#profile-pic {
			margin-top: -15%;
		}
		#margin-content {
			max-width: 1250px;
		}
		#top-card {
			height: 350px;
		}
		.card-block {
			min-height: 0;
			padding: 25px;
		}
		.card-title {
			margin-bottom: .75rem;
			font-size: 15px;
		}
		.card-text {
			font-size:13.5px;
			line-height: 1.6;
		}
		#name-content {
			margin-top: -9%;
			margin-bottom: 5%;
		}
		#name {
			font-weight: 300;
		}
		#location {
			font-weight: 200;
		}
		#change-cover {
			margin-top: -5%;
		}
		tr {
			border-top: 1px solid rgb(221, 221, 221);
		}
		.btn-white {
			color: #fff;
			border: 2px solid #fff;
			background: transparent;
		}
		.btn-white:hover {
			color:black;
			background: #fff;
			border: 2px solid #fff;
		}
		.profile-action {
			margin-top:-18%;
		}
		hr {
			margin-top: .25rem;
			margin-bottom: .25rem;
		}
	#headers
		#header-style-1
			.container
				.header-caption
					.row
						.col-md-12.header-content
							h3.header-title.animated.fadeInDown.invisible
								span.text-primary Helium - 
								| Bootstrap 4 UI Kit
							h5.header-text.animated.fadeIn.invisible
								| Lorem ipsum dolor sit amet, consectetuer adipiscing elit<br>Lorem ipsum dolor sit amet, consectetuer.</h5>
							//- a.page-scroll.btn.btn-lg.btn-default-filled.animated.fadeInUp.invisible(href="#") Download Now
							//- a.page-scroll.btn.btn-lg.btn-common.animated.fadeInUp.invisible(href="#") Explore
	#main-content
		#margin-content.container
			.row
				#profile-pic.col-md-3
					.card.shadow2
						img#top-card.card-img-top(src='/img/sampleprofpic.jpg', alt='')
						.profile-action
							.download.text-center
								button.btn.std-btn.btn-sm.btn-white(type='button')
									i.fa.fa-camera
									|  Change Picture
						.card-block
							h3.card-title.text-uppercase.text-primary About
							p.card-text.text-secondary Bacon ipsum dolor amet sirloin ball tip rump andouille burgdoggen, shoulder meatball shank doner shor, dolor amet sirloin ball dolor a sinta
				.col-md-9
					#name-content
						h1#name.text-white.mb-0 Eldrin Rei Del Rosario
						h6#location.text-white Marulas, Valenzuela City
						#change-cover.float-right.small-buttons-inner.mtb-50
							.download
								button.btn.std-btn.btn-sm.btn-white(type='button')
									i.fa.fa-pencil-square-o
