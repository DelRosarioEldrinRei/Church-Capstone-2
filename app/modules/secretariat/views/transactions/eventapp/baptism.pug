	script(type='text/javascript').
		$(document).ready(function(){
			for(var i =0; i<= $('.approvalreq').length; i++){
				if($('.approvalreq').eq(i).attr('value') == "Submitted"){
					$('.approvalreq').eq(i).addClass('badge btn btn-primary-lg bgc-amber-100 c-amber-700')
				}
				else if($('.approvalreq').eq(i).attr('value') == "Approved"){
					$('.approvalreq').eq(i).addClass('badge btn btn-primary-lg bgc-green-100 c-green-700')
				}
				else if($('.approvalreq').eq(i).attr('value') == "Disapproved"){
					$('.approvalreq').eq(i).addClass('badge btn btn-primary-lg bgc-red-100 c-red-700')
				}
				if($('.approvalpay').eq(i).attr('value') == "Paid"){
					$('.approvalpay').eq(i).addClass('badge btn btn-primary-lg bgc-green-100 c-green-700')
				}
				else if($('.approvalpay').eq(i).attr('value') == "Unpaid"){
					$('.approvalpay').eq(i).addClass('badge btn btn-primary-lg bgc-amber-100 c-amber-700')
				}
				if($('.approval').eq(i).attr('value') == "Pending"){
					$('.approval').eq(i).addClass('badge btn btn-primary-lg bgc-amber-100 c-amber-700')
				}
				else if($('.approval').eq(i).attr('value') == "Cancelled"){
					$('.approval').eq(i).addClass('badge btn btn-primary-lg bgc-grey-100 c-grey-700')
				}
				else if($('.approval').eq(i).attr('value') == "Approved"){
					$('.approval').eq(i).addClass('badge btn btn-primary-lg bgc-green-100 c-green-700')
				}
				else if($('.approval').eq(i).attr('value') == "Disapproved"){
					$('.approval').eq(i).addClass('badge btn btn-primary-lg bgc-red-100 c-red-700')
				}
			}

			var idNow;
			$('.actionButton').click(function(){
				idNow = $(this).val();
				console.log(idNow)
			});
			$(document).on('click', '.viewButton', function(){
				console.log(idNow)
				$.post('/secretariat/transaction-baptism/query',{id:idNow}).done(data =>{
					var dateRequested = moment(data.date_eventdate).format('MM/DD/YYYY')
					var timeRequested = moment(data.time_eventstart,'HH:mm:ss').format('hh:mm A')
					var age = moment().diff(data.date_birthday,'years');
					$('#viewBodyAppend').append(`
					<table class="tableModal">
						<tr>
							<td class="tdModal h5"><strong> Desired Date: </strong> ${dateRequested}</td>
							<td class="tdModal h5"><strong> Desired Time:  </strong> ${timeRequested}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Name of the Child: </strong> ${data.var_lname}, ${data.var_fname}</td>
							<td class="tdModal h5"><strong> Gender:  </strong> ${data.char_gender}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Relationship with the child:  </strong> ${data.var_relation}</td>
							<td class="tdModal h5"><strong> Contact No.:  </strong> ${data.var_contactnum}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Birth Date:  </strong> ${moment(data.date_birthday).format('YYYY-MM-DD')}</td>
							<td class="tdModal h5"><strong> Birth Place:  </strong> ${data.var_birthplace}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Mother's Name:  </strong> ${data.var_mothername}</td>
							<td class="tdModal h5"><strong> Mother's Birthplace:  </strong> ${data.var_motherbplace}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Father's Name:  </strong> ${data.var_fathername}</td>
							<td class="tdModal h5"><strong> Father's Birthplace:  </strong> ${data.var_fatherbplace}</td>
						</tr>
						<tr>
							<td class="tdModal h5"><strong> Address:  </strong> ${data.var_address}</td>
							<td class="tdModal h5"><strong> Parent Marriage Address:  </strong> ${data.var_parentmarriageadd}</td>
						</tr>
					`)
					$('#viewModal').modal('show')
					$('#viewModal').on('hidden.bs.modal',function(){
						$('#viewBodyAppend').empty()
					})

				})
			})
			$('.updateReqButton').click(function(){
				$.post('/secretariat/transaction-baptism/query/updateStatus',{id:idNow}).done(data=>{
					$('#updateReqModal').modal('show')
					var ctr=0;
					for(i=0;i<data.requirements.length;i++){
						if(data.requirements[i].var_reqstatus != "Approved"){
						$(`#reqAppend`).append(`
						<a class="image-link" href="${data.requirements[i].var_reqpath}" title="${data.requirements[i].var_reqname}"><img class="rounded shadow3" src="${data.requirements[i].var_reqpath}" style="width: 17rem;"/></a>
						<input class="custom-control-input" id="customRadio2" type="radio" name="Reject" value=${data.requirements[i].int_requirementID} />
						<input class="custom-control-input" id="customRadio2" type="radio" name="Approved" value=${data.requirements[i].int_requirementID} />
						`)
						}
						else if(data.requirements[i].var_reqstatus == "Approved"){
						ctr++;
						$(`#reqAppend`).append(`
						<a class="image-link" href="${data.requirements[i].var_reqpath}" title="${data.requirements[i].var_reqname}"><img class="rounded shadow3" src="${data.requirements[i].var_reqpath}" style="width: 17rem;"/></a>
						<input type="checkbox" disabled=true checked="" class="checkbox" value=${data.requirements[i].int_requirementID}>${data.requirements[i].var_reqname}  
						`) 
						}
					}
					if(ctr == data.requirements.length){
						$(`#reqStatus`).append(`
						<h6 class="label-control" class="reqstatus"><h5 class="d-inline">Current Requirement Status:</h5> <span class="approvalpay badge btn btn-primary-lg bgc-green-100 c-green-700" value="Complete">Complete</span>
						`)
					}
					else{
						$(`#reqStatus`).append(`
						<h6 class="label-control" class="reqstatus"><h5 class="d-inline">Current Requirement Status:</h5> <span class="approval badge btn btn-primary-lg bgc-amber-100 c-amber-700" value="Incomplete">Incomplete</span>
						`)
					}
					console.log(ctr)
					console.log(data.requirements.length)
					$('#updateReqButton').click(function(){
							swal({
							title:'Are You Sure?', 
							text:'You want to update the Status of the Requirements?', 
							type:'warning',
							showCancelButton: true,
							confirmButtonColor: '#3085d6',
							cancelButtonColor: '#d33',
							confirmButtonText: 'Confirm'
							}).then((result)=>{
							if(result.value){
							console.log($('#reqstatus').val())
							var reqstatus = $('#reqstatus').val()
							var paystatus = $('#paystatusupdate').val()
							for(i=0;i<$('.checkbox').length;i++){
							if($('.checkbox')[i].checked == true){
								$.post('/secretariat/transaction-baptism/updateRequirements',{
								id:$('.checkbox')[i].getAttribute('value'),
								eventid:idNow
								})
								}
							}
							swal({
								title:'Success',
								text:'Status updated', 
								type:'success'
								}).then(function(){
									location.reload();
								})
							}
							})
					})
					$('#updateReqModal').on('hidden.bs.modal',function(){
						$('#reqAppend').empty()
						$('#reqStatus').empty()
					})
				})
			})
			
			$('.updateButton').click(function(){
				$.post('/secretariat/transaction-baptism/query/updateStatus',{id:idNow}).done(data=>{
				$('#updateBodyAppend').append(`
				<col-md-4 class="p-lg-0">
						<div class="row">
						<div class="col-md-6">
							<div class="form-group">
							<input id="eventid" type="hidden" value="${data.results.int_eventinfoID}"></input>
							<input id="userid" type="hidden" value="${data.results.int_userID}"></input>
							<h6 class="label-control" id="eventstatus">
							</h6>
						</div>
						</div>
						</div>
						<div class="row">
						<div class="col-md-6">
							<div class="form-group">
							<h6 class="label-control" id="reqstatus">
							</h6>
							</div>
						</div>
						<div class="col-md-12">
							<div class="form-group">
							<input id="payid" type="hidden" value="${data.results.int_paymentID}"></input>
							<h6 class="label-control" id="paystatus">
							</h6>
